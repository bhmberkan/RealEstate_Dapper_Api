@model List<ResultWriteUsInformationDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Admin/Index.cshtml";
}




<link href="~/css/paging.css" rel="stylesheet" />

<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
            <div class="bg-light rounded h-100 p-4">
                <h6 class="mb-4">Kullanıcı Listesi</h6>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                              
                                <th scope="col">Tel1</th>
                                <th scope="col">Tel2</th>
                                <th scope="col">Email</th>
                                <th scope="col">Adres</th>
                                <th scope="col">Sil</th>
                                <th scope="col">Güncelle</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                
                                <tr>
                                   
                                    <td>@item.Tel1</td>
                                    <td>@item.Tel2</td>
                                    <td>@item.Email</td>
                                    <td>@item.Address</td>
                                    <td><a href="/WriteUsInformation/DeleteWriteUsInformation/@item.WriteUsInformationId" class="btn btn-outline-danger">Sil</a></td>
                                    <td><a href="/WriteUsInformation/UpdateWriteUsInformation/@item.WriteUsInformationId" class="btn btn-outline-success">Güncelle</a></td>


                                </tr>
                            }
                        </tbody>
                    </table>
                @*     @Html.PagedListPager((IPagedList)Model, sayfa => Url.Action("Index", new { sayfa }))
                    <a href="/WriteUsInformation/CreateWriteUsInformation/" class="btn btn-outline-info">İnfto Girişi</a> *@
                </div>
            </div>
        </div>
    </div>
</div>